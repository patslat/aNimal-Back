<div class="game">
  <div class="board">
    <div class="row-fluid span12">
      <div id="block-0" class="offset1 block span3"></div>
      <div id="block-1" class="block span3"></div>
      <div id="block-2" class="block span3"></div>
    </div>
    <div class="row-fluid span12">
      <div id="location-indicator" class="indicator span1">
        <div class="icon">
          <i id="visual-icon" class="icon-eye-open"></i>
        </div>
      </div>
      <div id="block-3" class="block span3"></div>
      <div class="block center span3">
        <div id="status"></div>
      </div>
      <div id="block-4" class="block span3"></div>

      <div id="auditory-indicator" class="indicator span1">
        <div class="icon">
          <i id="auditory-icon" class="icon-headphones"></i>
        </div>
      </div>
    </div>
  </div>

  <div class="row-fluid span12">
      <div id="block-5" class="block offset1 span3"></div>
      <div id="block-6" class="block span3"></div>
      <div id="block-7" class="block span3"></div>
  </div>

  <div class="start-button">
    <div class="row-fluid span12">
      <button id="start" class="btn offset4 span3">Play</button>
    </div>
  </div>
</div>

<div id="stats" class="row-fluid span12">
</div>

